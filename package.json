  "license": "MIT",
  "dependencies": {
    "@replit/database": "latest",
    "aes-js": "latest",
    "axios": "latest",
    "bluebird": "^2.11.0",
    "chalk": "4.1.2",
    "cheerio": "latest",
    "crypto": "latest",
    "gettext.js": "^1.1.1",
